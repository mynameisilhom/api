        "/api/v1/register": {
            "post": {
                "tags": [
                    "Avtorizatsiya"
                ],
                "summary": "Ro'yhatdan o'tish",
                "operationId": "26ac06b496123d055b26a788165924bb",
                "requestBody": {
                    "description": "Ro'yhatdan o'tish so'rovi",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "name": {
                                        "description": "FIO",
                                        "type": "string",
                                        "example": "John Doe"
                                    },
                                    "email": {
                                        "description": "Foydalanuvchi Emaili",
                                        "type": "string",
                                        "format": "email",
                                        "example": "john@example.com"
                                    },
                                    "password": {
                                        "description": "Foydalanuvchi paroli",
                                        "type": "string",
                                        "example": "secret"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Foydalanuvchi qo'shildi"
                    }
                }
            }
        },
        "/api/v1/login": {
            "post": {
                "tags": [
                    "Avtorizatsiya"
                ],
                "summary": "Saytga kirish",
                "operationId": "3e5fe318fe58355689b1608ed9f5430b",
                "requestBody": {
                    "description": "Kirish so'rovi",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "email": {
                                        "description": "Foydalanuvchi Emaili",
                                        "type": "string",
                                        "format": "email",
                                        "example": "john@example.com"
                                    },
                                    "password": {
                                        "description": "Foydalanuvchi paroli",
                                        "type": "string",
                                        "example": "secret"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Kirish amalga oshirildi"
                    }
                }
            }
        },
        "/api/v1/logout": {
            "get": {
                "tags": [
                    "Avtorizatsiya"
                ],
                "summary": "Saytdan chiqish",
                "operationId": "13b9babde691ed66139e55f82d56898b",
                "requestBody": {
                    "description": "Chiqish so'rovi",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "token": {
                                        "description": "Foydalanuvchi tokeni",
                                        "type": "string",
                                        "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6MTIzNDU2Nzg5MCwiaWF0IjoxNTE2MjM5MDIyfQ.SflKxwRJSMeKKF2QT4fwpMeJf36POk6yJV_adQssw5c"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Chiqish amalga oshirildi"
                    }
                }
            }
        },
        "/api/v1/categories": {
