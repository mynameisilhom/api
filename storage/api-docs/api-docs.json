                "tags": [
                    "Turkumlar"
                ],
                "summary": "Turkum ochish",
                "operationId": "0c5854a86978e3decf9e294f48fb428d",
                "requestBody": {
                    "description": "Turkum ochish",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "title"
                                ],
                                "properties": {
                                    "title": {
                                        "type": "string",
                                        "maxLength": 255,
                                        "minLength": 3,
                                        "example": "Turkum nomi"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Turkum muvaffaqiyatli ochildi"
                    },
                    "401": {
                        "description": "Avtorizatsiyadan o'tilmagan"
                    },
                    "422": {
                        "description": "Noto'g'ri ma'lumotlar"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
